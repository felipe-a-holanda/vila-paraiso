[Unit]
Description=gunicorn daemon
After=network.target

[Service]
User=django
Group=django
WorkingDirectory=/home/django/vilaparaiso.org
ExecStart=/home/django/vilaparaiso.org/env/bin/gunicorn --access-logfile - --workers 3 --bind unix:/home/django/vilaparaiso.org/myproject.sock vilaparaiso.wsgi:application

[Install]
WantedBy=multi-user.target
